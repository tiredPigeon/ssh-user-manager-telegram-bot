{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "esnext",
      "dom"
    ],
    "module": "commonjs" ,
    "moduleResolution": "node",
    "resolveJsonModule": true ,
    "sourceMap": true ,
    "outDir": "dist" ,
    "esModuleInterop": true ,
    "forceConsistentCasingInFileNames": true ,
    "strict": true ,
    "skipLibCheck": true,
    "pretty": true
  },
  "exclude": ["node_modules"]
}
